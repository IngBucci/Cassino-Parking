{{#if msg_successo}}
    <div class="alert alert-success alert-dismissible fade show">{{testo}}
        <button class="close" data-dismiss="alert"><span>&times;</span></button>
    </div>
{{/if}}

{{#if msg_errore}}
    <div class="alert alert-danger alert-dismissible fade show">{{testo}}
        <button class="close" data-dismiss="alert"><span>&times;</span></button>
    </div>
{{/if}}